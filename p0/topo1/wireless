
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'virtual/uccp420/uccwlan'
	option country '00'
	option channel '11'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option encryption 'psk2'
	option mode 'sta'
	option ssid 'Pto0919'
	option key 'abcd1234'

